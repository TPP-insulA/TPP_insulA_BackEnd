{
  "name": "tpp_insula_backend",
  "version": "1.0.0",
  "description": "Backend para aplicaci√≥n de control de insulina",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "rimraf dist && tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"src/**/*.d.ts\" dist/ && copyfiles -u 1 \"prisma/**/*\" dist/",
    "prepare": "npx prisma generate",
    "deploy": "npm run build && npm run prepare && npm start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "postinstall": "npm run prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franciscoduc4/TPP_insulA_BackEnd.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/franciscoduc4/TPP_insulA_BackEnd/issues"
  },
  "homepage": "https://github.com/franciscoduc4/TPP_insulA_BackEnd#readme",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "bcryptjs": "^2.4.3",
    "clarifai": "^2.9.1",
    "clarifai-nodejs-grpc": "^11.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.1.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "copyfiles": "^2.4.1",
    "prisma": "^4.15.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
